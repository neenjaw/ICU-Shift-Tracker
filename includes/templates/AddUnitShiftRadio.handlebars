<div class="form-group">
  {{#each staff}}
    <div class="form-check">
      <label class="form-check-label">
        <input class="form-check-input" type="radio" name="{{../prefix}}" id="{{../prefix}}-{{../type}}-{{this.id}}" value="{{this.value}}">
        {{this.name}}
      </label>
    </div>
  {{/each}}
</div>
