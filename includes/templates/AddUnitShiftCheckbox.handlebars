<div class="form-group">
  {{#each item}}
    <div class="form-check">
      <label class="form-check-label">
        <input name="{{../prefix}}" id="{{this.prefix}}-{{this.type}}-{{this.id}}" class="form-check-input" type="checkbox" value="{{this.value}}">
        {{this.name}}
      </label>
    </div>
  {{/each}}
</div>
