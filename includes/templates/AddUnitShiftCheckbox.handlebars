<div class="form-group">
  {{#each staff}}
    <div class="form-check">
      <label class="form-check-label">
        <input name="{{../prefix}}" id="{{../prefix}}-{{../type}}-{{this.id}}" class="form-check-input" type="checkbox" value="{{this.id}}">
        {{this.name}}
      </label>
    </div>
  {{/each}}
</div>
