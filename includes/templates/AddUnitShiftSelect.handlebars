<div class="form-group">
  <label for="exampleFormControlSelect2">Choose staff for {{label}}</label>
  <select multiple class="form-control" name="{{prefix}}" id="{{prefix}}-{{type}}">
    {{#each staff}}
      <option id="{{../prefix}}-{{../type}}-{{this.id}}" value="{{this.id}}">{{this.name}}</option>
    {{/each}}
  </select>
</div>
